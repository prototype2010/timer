{
  "name": "brocoders-timer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://prototype2010.github.io/timer/",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "connected-react-router": "^6.4.0",
    "gh-pages": "^2.0.1",
    "history": "latest",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "randomstring": "^1.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "5.0.1",
    "recharts": "^1.5.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "prettier": "prettier --write --jsx-single-quote --trailing-comma none --use-tabs false --single-quote src/**/*.{js,jsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jest": "^24.7.1",
    "prettier": "1.17.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write --jsx-single-quote --trailing-comma none --use-tabs false --single-quote src/**/*.{js,jsx}",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
